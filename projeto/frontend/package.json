{
    "name": "hospital-frontend",
    "version": "1.0.0",
    "scripts": {
        "test": "jest",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx && prettier --check src/",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "eslint": "^8.50.0",
        "prettier": "^3.0.0",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "@types/jest": "^29.5.0",
        "typescript": "^5.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm test"
        }
    }
}